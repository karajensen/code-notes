	if ($Undo_Search == "Fracture_Seperate()")
		{
			undo;
			Fracture_Seperate();
		}
	if ($Undo_Search == "Fracture_RE_Seperate()")
		{
			undo;
			Fracture_Seperate();
		}
	*/
}
