	string $Base_Obj[] = $Sel_Objs	;
	for ($c = 1; $c < `size Sel_Objs`; $c++)
				{
					if (`objExists $Base_Obj[0]` && `objExists $Sel_Objs[$c]`)
						{
							$Base_Obj = `polyBoolOp -op 1 -ch 0 $Base_Obj[0] $Sel_Objs[$c]`;
