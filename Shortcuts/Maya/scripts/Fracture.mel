global proc Fracture()
{

	$UI_Width = 250;
	if ((`window -ex  Fracture_UI`) == true) deleteUI Fracture_UI;
	$Fracture_Window = `window -title "Fracture V 4.7" -w $UI_Width Fracture_UI`;
	Build_Fracture_UI (1);

	showWindow Fracture_UI;
}

